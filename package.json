{
  "version": "0.1.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "types",
    "templates"
  ],
  "browser": {
    "fs": false
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "start:dev": "cross-env APP_ROOT=example umi dev",
    "build": "tsdx build",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@asany/umi-plugin-apollo",
  "author": "李茂峰",
  "repository": "https://github.com/limaofeng/umi-plugin-apollo",
  "module": "dist/umi-plugin-apollo.esm.js",
  "size-limit": [
    {
      "path": "dist/umi-plugin-apollo.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/umi-plugin-apollo.esm.js",
      "limit": "100 KB"
    }
  ],
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@lifeomic/axios-fetch": "^2.0.5",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-ws": "^1.0.20",
    "apollo-upload-client": "^16.0.0",
    "axios": "^0.24.0",
    "graphql": "^15.5.1",
    "lodash": "^4.17.21",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@umijs/types": "^3.5.17",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "pinst": "^2.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.3",
    "test-umi-plugin": "^0.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "umi": "^3.5.17",
    "umi-test": "^1.9.6"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
