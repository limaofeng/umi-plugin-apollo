{
  "name": "@asany/umi-plugin-apollo",
  "author": "李茂峰 <limaofeng@msn.com>",
  "version": "0.2.1",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "father dev",
    "build": "father build",
    "example": "APP_ROOT=example umi dev",
    "lint": "umi lint --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "lint:prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql,gql}\" \"templates/**/*.{ts,tsx,graphql,gql}\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*.{ts,tsx,graphql,gql}\" \"templates/**/*.{ts,tsx,graphql,gql}\""
  },
  "keywords": [],
  "authors": {
    "name": "李茂峰 <limaofeng@msn.com>",
    "email": "limaofeng@msn.com"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.15.11",
    "conventional-changelog-cli": "^4.1.0",
    "father": "^4.1.8",
    "husky": "^7.0.2",
    "prettier": "^3.2.5",
    "size-limit": "^5.0.3",
    "typescript": "^5.0.3",
    "umi": "^4.1.2"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@lifeomic/axios-fetch": "^3.0.1",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-logger": "^2.0.1",
    "apollo-upload-client": "^16.0.0",
    "axios": "^1.6.7",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist",
      "limit": "100 KB",
      "webpack": false,
      "config": "size-limit-webpack.config.js"
    }
  ]
}
